{
  "name": "intervaltree",
  "version": "0.0.1",
  "description": "Interval tree for JavaScript",
  "author": "Jordan Baker",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "install": "tsc || true",
    "build": "tsc || true",
    "prebuild": "rimraf dist",
    "test": "jest",
    "test:ci": "jest --ci --testResultsProcessor='jest-junit'",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "coverage:watch": "jest --coverage --watch",
    "coverage:open": "open coverage/lcov-report/index.html",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hexsprite/intervaltree.git"
  },
  "keywords": [
    "typescript",
    "library",
    "intervaltree",
    "tree"
  ],
  "bugs": {
    "url": "https://github.com/hexsprite/intervaltree/issues"
  },
  "homepage": "https://github.com/hexsprite/intervaltree",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "rimraf": "^2.6.3",
    "standard-version": "^6.0.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-microsoft-contrib": "^6.2.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.135",
    "assert": "^2.0.0",
    "collections": "git+https://github.com/Cornally/collections.git",
    "debug": "^4.1.1",
    "lodash": "^4.17.11",
    "lodash.range": "^3.2.0",
    "typescript": "^3.5.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testRegex": ".*\\.(test|spec)\\.(ts)$",
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    }
  }
}
