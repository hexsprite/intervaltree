{
  "name": "intervaltree",
  "version": "0.0.1",
  "description": "Interval tree for JavaScript",
  "author": "Jordan Baker",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "install": "tsc || true",
    "build": "tsc || true",
    "prebuild": "rimraf dist",
    "test": "jest",
    "test:ci": "jest --ci --testResultsProcessor='jest-junit'",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "coverage:watch": "jest --coverage --watch",
    "coverage:open": "open coverage/lcov-report/index.html",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hexsprite/intervaltree.git"
  },
  "keywords": ["typescript", "library", "intervaltree", "tree"],
  "bugs": {
    "url": "https://github.com/hexsprite/intervaltree/issues"
  },
  "homepage": "https://github.com/hexsprite/intervaltree",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.3",
    "jest": "^23.0.1",
    "jest-junit": "^4.0.0",
    "rimraf": "^2.6.2",
    "standard-version": "^4.4.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-microsoft-contrib": "^5.0.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.109",
    "assert": "^1.4.1",
    "collections": "git+https://github.com/Cornally/collections.git",
    "debug": "^3.1.0",
    "lodash": "^4.17.10",
    "lodash.range": "^3.2.0",
    "typescript": "^2.8.3"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx"],
    "testRegex": ".*\\.(test|spec)\\.(ts)$",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": ["src/**/*.{ts,tsx}", "!src/**/*.d.ts"]
  }
}
