{
  "name": "intervaltree",
  "version": "0.0.1",
  "description": "Interval tree for JavaScript",
  "author": "Jordan Baker",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "install": "tsc || true",
    "build": "tsc || true",
    "prebuild": "rimraf dist",
    "test": "jest",
    "test:ci": "jest --ci --testResultsProcessor='jest-junit'",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "coverage:watch": "jest --coverage --watch",
    "coverage:open": "open coverage/lcov-report/index.html",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hexsprite/intervaltree.git"
  },
  "keywords": [
    "typescript",
    "library",
    "intervaltree",
    "tree"
  ],
  "bugs": {
    "url": "https://github.com/hexsprite/intervaltree/issues"
  },
  "homepage": "https://github.com/hexsprite/intervaltree",
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.28",
    "jest": "^21.0.2",
    "jest-junit": "^3.0.0",
    "rimraf": "^2.5.2",
    "standard-version": "^4.2.0",
    "ts-jest": "^21.0.0",
    "tslint": "^5.7.0",
    "tslint-microsoft-contrib": "^5.0.1"
  },
  "dependencies": {
    "@types/lodash": "^4.14.74",
    "assert": "^1.4.1",
    "collections": "git+https://github.com/Cornally/collections.git",
    "debug": "^3.0.1",
    "lodash": "^4.17.3",
    "lodash.range": "^3.2.0",
    "typescript": "^2.5.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testRegex": ".*\\.(test|spec)\\.(ts)$",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "mapCoverage": true
  }
}
