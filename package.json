{
  "name": "intervaltree",
  "version": "0.0.1",
  "description": "Interval tree for JavaScript",
  "author": "Jordan Baker",
  "license": "MIT",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "install": "tsc --declaration || true",
    "build": "tsc --declaration || true",
    "prebuild": "rimraf dist",
    "test": "jest",
    "test:ci": "jest --ci --testResultsProcessor='jest-junit'",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "coverage:watch": "jest --coverage --watch",
    "coverage:open": "open coverage/lcov-report/index.html",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hexsprite/intervaltree.git"
  },
  "keywords": [
    "typescript",
    "library",
    "intervaltree",
    "tree"
  ],
  "bugs": {
    "url": "https://github.com/hexsprite/intervaltree/issues"
  },
  "homepage": "https://github.com/hexsprite/intervaltree",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.12",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "rimraf": "^3.0.0",
    "standard-version": "^7.0.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-microsoft-contrib": "^6.2.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.144",
    "assert": "^2.0.0",
    "collections": "git+https://github.com/Cornally/collections.git",
    "crypto": "^1.0.1",
    "debug": "^4.1.1",
    "lodash": "^4.17.15",
    "lodash.range": "^3.2.0",
    "typescript": "^3.6.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testRegex": ".*\\.(test|spec)\\.(ts)$",
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    }
  }
}
